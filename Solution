
// ***  sumArray() ***
Write a function sumArray() that accepts an array of numbers and 
returns the sum of all numbers in the array.

function sumArray(arr) {
    var total = 0;
    arr.forEach(function(element){
        total +=element;
    });
    return total;
}//enter
undefined
sumArray([29,23,25])//enter
77

// NEW solution for the sumArray()
// Write a function sumArray() that accepts an array of numbers and 
// returns the sum of all numbers in the array

var sum = [1, 2, 3].reduce(add, 0);

function add(a, b) {
    return a + b;
}

console.log(sum);//enter
6
undefined

// another example above topic


function sumArray(arr) {
  console.log(arr.reduce((accu, val) => accu + val));
}

sumArray([5,4,3]);//enter
12
undefined


